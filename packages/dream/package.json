{
	"name": "dream",
	"version": "0.0.0",
	"type": "module",
	"license": "ISC",
	"types": "dist/dream.d.ts",
	"main": "dist/dream.js",
	"exports": {
		".": {
			"types": "./dist/dream.d.ts",
			"node": {
				"module-sync": "./dist/dream.js",
				"default": "./dist/dream.js"
			},
			"default": "./dist/dream.js"
		},
		"./browser": {
			"types": "./dist/dream-browser.d.ts",
			"browser": {
				"module-sync": "./dist/dream-browser.js",
				"default": "./dist/dream-browser.js"
			},
			"default": "./dist/dream-browser.js"
		},
		"./jsx": {
			"types": "./dist/dream-jsx.d.ts",
			"node": {
				"module-sync": "./dist/dream-jsx.js",
				"default": "./dist/dream-jsx.js"
			},
			"default": "./dist/dream-jsx.js"
		},
		"./jsx-runtime": {
			"types": "./dist/dream-jsx.d.ts",
			"node": {
				"module-sync": "./dist/dream-jsx.js",
				"default": "./dist/dream-jsx.js"
			},
			"default": "./dist/dream-jsx.js"
		},
		"./jsx-dev-runtime": {
			"types": "./dist/dream-jsx.d.ts",
			"node": {
				"module-sync": "./dist/dream-jsx.js",
				"default": "./dist/dream-jsx.js"
			},
			"default": "./dist/dream-jsx.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsup",
		"build-example": "vite build --app",
		"dev": "vite",
		"format": "biome check --write .",
		"test": "vitest"
	},
	"dependencies": {
		"@babel/core": "7.26.0",
		"@biomejs/biome": "1.9.4",
		"@mjackson/node-fetch-server": "0.1.0",
		"pipeable-dom": "0.0.26",
		"std-router": "0.0.9",
		"ts-toolbelt": "9.6.0",
		"urlpattern-polyfill": "10.0.0",
		"vite": "6.0.0-beta.5"
	},
	"devDependencies": {
		"@types/babel__core": "7.20.5",
		"@types/dom-view-transitions": "1.0.5",
		"@types/node": "^20",
		"tsup": "8.3.5",
		"typescript": "5.6.3",
		"vite-tsconfig-paths": "5.0.1",
		"vitest": "2.1.4"
	},
	"pnpm": {
		"overrides": {
			"vite": "6.0.0-beta.5"
		}
	}
}
